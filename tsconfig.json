{
    "compilerOptions": {
        "target": "ES2022",
        "lib": ["ES2022", "ES2023", "ES2024"],
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "allowImportingTsExtensions": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noEmit": true,
        "skipLibCheck": true
    }
}
